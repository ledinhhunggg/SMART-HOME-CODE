   "source": [
    "from pop import PwmController\n",
    "import time\n",
    "\n",
    "pwm = PwmController()\n",
    "pwm.init()\n",
    "pwm.setChannel(3)\n",
    "pwm.setFreq(50)\n",
    "while True:\n",
    "    pwm.setDuty((0.5/20)*100)\n",
    "    time.sleep(3)\n",
    "    pwm.setDuty((1.5/20)*100)\n",
    "    time.sleep(3)\n",
    "    pwm.setDuty((2.5/20)*100)\n",
    "    time.sleep(3)\n"
   ]
